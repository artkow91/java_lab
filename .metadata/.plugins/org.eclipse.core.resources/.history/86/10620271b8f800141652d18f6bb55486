abstract class FiguraGeometryczna {
	
	private String typ_figury;

	abstract String dajTyp();
	abstract double dajPole();
//	int[] dajIloscParamterow();

	abstract String dajTyp() {

	}
	abstract double dajPole(){
		
	}
	int[] dajIloscParametrow(){
		return {1};
	}

	public void ustawParametry(double[] param, String opis) {
		typ_figury = opis;
		
	}

}
